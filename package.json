    "@babel/preset-react": "7.16.0",
    "clean-webpack-plugin": "1.0.0",
    "css-loader": "1.0.1",
    "css-minimizer-webpack-plugin": "3.0.2",
    "formik": "2.2.9",
    "html-loader": "0.5.5",
